    <IsTestProject>true</IsTestProject>
    <Nullable>enable</Nullable>
    <RootNamespace>ptr727.LanguageTags.Tests</RootNamespace>
    <TargetFramework>net10.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
